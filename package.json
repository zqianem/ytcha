{
  "name": "ytcha",
  "version": "1.0.0",
  "description": "CLI that lists the latest videos from a YouTube channel",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun run ./index.ts",
    "dist": "bun build ./index.ts --target node --outfile dist/index.js",
    "local-install": "bun build ./index.ts --compile --outfile ~/.local/bin/ytcha"
  },
  "keywords": ["youtube", "channel", "videos", "list"],
  "author": "Em Zhan",
  "license": "MIT",
  "packageManager": "bun",
  "dependencies": {
    "commander": "^12.1.0",
    "puppeteer": "^23.4.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/which": "^3.0.4"
  }
}
